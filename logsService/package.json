{
  "name": "logsService",
  "version": "1.0.0",
  "description": "Comprehensive logging service for Cloudflare Workers with Tail Worker integration",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "cf-typegen": "wrangler types",
    "migrations:create": "wrangler d1 migrations create platform-logs-db",
    "migrations:apply": "wrangler d1 migrations apply platform-logs-db",
    "build:frontend": "cd frontend && npm run build",
    "dev:frontend": "cd frontend && npm run dev"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "logging",
    "tail-worker",
    "observability"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.5.0",
    "@cloudflare/workers-types": "^4.20241106.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8",
    "wrangler": "^3.90.0"
  },
  "dependencies": {
    "hono": "^4.6.14",
    "date-fns": "^3.0.0",
    "date-fns-tz": "^2.0.0",
    "zod": "^3.22.4"
  }
}
